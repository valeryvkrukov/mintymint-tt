<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <base href="{{ path('homepage') }}">
        <title>{% block title %}mURL{% endblock %}</title>
        {% block stylesheets %}
	{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}">
    </head>
    <body>
        {% block body %}{% endblock %}
    </body>
    {% block javascripts %}
    <script type="text/javascript" src="{{ asset('bundles/app/mURL/dist/inline.bundle.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/app/mURL/dist/polyfills.bundle.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/app/mURL/dist/styles.bundle.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/app/mURL/dist/vendor.bundle.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/app/mURL/dist/main.bundle.js') }}"></script>
    {% endblock %}
</html>
